{
    "name": "kerryjones/bulma-flash",
    "description": "Easy Laravel/Bulma flash notifications",
    "license": "MIT",
    "authors": [
        {
            "name": "Kerry Jones",
            "email": "kerryjones21@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-0": {
            "KerryJones\\Flash": "src/"
        },
        "files": [
            "src/KerryJones/Flash/functions.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "KerryJones\\Flash\\FlashServiceProvider"
            ],
            "aliases": {
                "Flash": "KerryJones\\Flash\\Flash"
            }
        }
    }
}
